{
  "name": "sanity-plugin-content-calendar",
  "version": "2.0.0-v3-studio.0",
  "description": "A Content Calendar for your Sanity Studio",
  "source": "./src/index.tsx",
  "exports": {
    ".": {
      "require": "./lib/cjs/index.js",
      "default": "./lib/esm/index.js"
    }
  },
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/types/index.d.ts",
  "files": [
    "src",
    "lib",
    "v2-incompatible.js",
    "sanity.json"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "prebuild": "npm run clean && plugin-kit verify-package --silent",
    "build": "parcel build --no-cache",
    "watch": "parcel watch",
    "link-watch": "plugin-kit link-watch",
    "prepublishOnly": "npm run build",
    "compile": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier src -w",
    "prepare": "husky install"
  },
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "dependencies": {
    "@sanity/icons": "^1.3.0",
    "@sanity/incompatible-plugin": "^0.0.1-studio-v3.1",
    "@sanity/ui": "^0.37.11",
    "date-fns": "^2.28.0",
    "dlv": "^1.1.3",
    "react-big-calendar": "^0.40.1",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@parcel/packager-ts": "^2.6.0",
    "@parcel/transformer-typescript-types": "^2.6.0",
    "@sanity/plugin-kit": "^0.1.0-v3-studio.1",
    "@types/dlv": "^1.1.2",
    "@types/react-big-calendar": "^0.38.0",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "autoprefixer": "^10.2.4",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-sanity": "^6.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.4.1",
    "microbundle": "^0.14.2",
    "parcel": "^2.6.0",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0",
    "rimraf": "^3.0.2",
    "sanity": "^3.0.0-dev-preview.8",
    "typescript": "^4.6.3"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0",
    "sanity": "dev-preview"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
